---
BasedOnStyle: Google
IndentWidth: 4

AlignOperands: Align
BreakBeforeBraces: Custom
BraceWrapping:
    AfterCaseLabel: true
    AfterClass: true
    AfterControlStatement: Always
    AfterEnum: true
    AfterFunction: true
    AfterNamespace: true
    AfterStruct: true
    AfterUnion: true
    AfterExternBlock: true
    BeforeCatch: true
    BeforeElse: true
    BeforeLambdaBody: true
    BeforeWhile: true
    IndentBraces: false
    SplitEmptyRecord: false
    SplitEmptyFunction: false
ColumnLimit: 200
IndentAccessModifiers: true
InsertNewlineAtEOF: true
SeparateDefinitionBlocks: Always
---
